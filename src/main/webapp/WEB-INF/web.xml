<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Project 1</display-name>
   <servlet>
  <servlet-name>myLogin</servlet-name>
  <servlet-class>com.login.Login_Logic</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>myLogin</servlet-name>
  <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  
   
  <servlet>
  <servlet-name>DbTable</servlet-name>
  <servlet-class>com.login.Creat_Table</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>DbTable</servlet-name>
  <url-pattern>/DBTable</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>SignUp</servlet-name>
  <servlet-class>com.login.Registration_Logic</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>SignUp</servlet-name>
  <url-pattern>/Register</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Verify</servlet-name>
  <servlet-class>com.login.User_Verfication</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Verify</servlet-name>
  <url-pattern>/VPass</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>ChangePass</servlet-name>
  <servlet-class>com.login.Reset_Password</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>ChangePass</servlet-name>
  <url-pattern>/CPass</url-pattern>
  </servlet-mapping>
  
</web-app>